<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>
  <schedule>
    <job>
      <name>GeneratedActiveReportJob</name>
      <group>Group1</group>
      <description>生成白名单服务</description><!--实现IJob的程序集和具体类-->
      <job-type>AttackPrevent.WindowsService.Job.GeneratedActiveReportJob, AttackPrevent.WindowsService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>GeneratedActiveReportTrigger</name>
        <group>Group1</group>
        <job-name>GeneratedActiveReportJob</job-name>
        <job-group>Group1</job-group><!--0 0/5 8-16 * * ?|*/5 * * * * ?-->
        <cron-expression>0 0/5 8-16 * * ?</cron-expression>
      </cron>
    </trigger>
    <job>
      <name>EmailSendJob</name>
      <group>Group1</group>
      <description>发送邮件服务</description><!--实现IJob的程序集和具体类-->
      <job-type>AttackPrevent.WindowsService.Job.EmailSendJob, AttackPrevent.WindowsService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>EmailSendJobTrigger</name>
        <group>Group1</group>
        <job-name>EmailSendJob</job-name>
        <job-group>Group1</job-group><!--0 0/15 9-18 * * ?|*/5 * * * * ?-->
        <cron-expression>0 0/15 9-18 * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>