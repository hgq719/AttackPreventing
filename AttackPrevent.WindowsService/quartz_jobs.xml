<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>
  <schedule>
    <job>
      <name>EmailSendJob</name>
      <group>Group1</group>
      <description>定时发送邮件服务</description>
      <!--实现IJob的程序集和具体类-->
      <job-type>AttackPrevent.WindowsService.Job.EmailSendJob, EmailSendJob</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>Trigger1</name>
        <group>Group1</group>
        <job-name>EmailSendJob</job-name>
        <job-group>Group1</job-group>
        <cron-expression>0 0 9 * * ?</cron-expression>
      </cron>
    </trigger>
    <job>
      <name>LogAnalyzeJob</name>
      <group>Group2</group>
      <description>日志分析服务</description>
      <!--实现IJob的程序集和具体类-->
      <job-type>AttackPrevent.WindowsService.Job.LogAnalyzeJob, LogAnalyzeJob</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>Trigger2</name>
        <group>Group2</group>
        <job-name>LogAnalyzeJob</job-name>
        <job-group>Group2</job-group>
        <cron-expression>0 0/10 * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>